{
    "manufacturer": "Thibault\u202fPolge",
    "keyboard_name": "habilis_flat",
    "maintainer": "thblt",
    "bootloader": "atmel-dfu",
    "diode_direction": "COL2ROW",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "nkro": true
    },
    "matrix_pins": {
        "cols": ["B5", "B4", "E6", "D7", "C6", "D4", "F6"],
        "rows": ["B6", "B2", "B3", "B1", "F7"]
    },
    "processor": "atmega32u4",
    "url": "",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "layouts": {
        "LAYOUT": {
            // Symmetrical split keyboard. Notice that the thumb
            // cluster has six keys, so it also contains the last key
            // of the previous (innermost, on pinkies) column, which
            // has one key less than regular columns.
            "layout": [
                /* Left hand */
                // row 1 (numbers)
                { "matrix": [0,0], "x": 0, "y": 0 },
                { "matrix": [0,1], "x": 1, "y": 0 },
                { "matrix": [0,2], "x": 2, "y": 0 },
                { "matrix": [0,3], "x": 3, "y": 0 },
                { "matrix": [0,4], "x": 4, "y": 0 },
                { "matrix": [0,5], "x": 5, "y": 0 },
                // row 2 (QWERT…)
                { "matrix": [1,0], "x": 0, "y": 1 },
                { "matrix": [1,1], "x": 1, "y": 1 },
                { "matrix": [1,2], "x": 2, "y": 1 },
                { "matrix": [1,3], "x": 3, "y": 1 },
                { "matrix": [1,4], "x": 4, "y": 1 },
                { "matrix": [1,5], "x": 5, "y": 1 },
                // row 3 (ASDFG…)
                { "matrix": [2,0], "x": 0, "y": 2 },
                { "matrix": [2,1], "x": 1, "y": 2 },
                { "matrix": [2,2], "x": 2, "y": 2 },
                { "matrix": [2,3], "x": 3, "y": 2 },
                { "matrix": [2,4], "x": 4, "y": 2 },
                { "matrix": [2,5], "x": 5, "y": 2 },
                // row 4 (ZXCVB…)
                { "matrix": [3,0], "x": 0, "y": 3 },
                { "matrix": [3,1], "x": 1, "y": 3 },
                { "matrix": [3,2], "x": 2, "y": 3 },
                { "matrix": [3,3], "x": 3, "y": 3 },
                { "matrix": [3,4], "x": 4, "y": 3 },
                { "matrix": [3,5], "x": 5, "y": 3 },
                // row 5 (ASDFG…)
                { "matrix": [4,0], "x": 0, "y": 4 },
                { "matrix": [4,1], "x": 1, "y": 4 },
                { "matrix": [4,2], "x": 2, "y": 4 },
                { "matrix": [4,3], "x": 3, "y": 4 },
                { "matrix": [4,4], "x": 4, "y": 4 },
                // Left thumb cluster
                { "matrix": [4,5], "x": 5, "y": 4 },
                { "matrix": [0,6], "x": 6, "y": 0 },
                { "matrix": [1,6], "x": 6, "y": 1 },
                { "matrix": [2,6], "x": 6, "y": 2 },
                { "matrix": [3,6], "x": 6, "y": 3 },
                { "matrix": [4,6], "x": 6, "y": 4 },

                /* Right hand.
                 *
                 * Notice that since the PCB is reverted on that hand,
                 * columns go right to left (last column is the
                 * innermost one).  */
                { "matrix": [5,5], "x": 7, "y": 0 },
                { "matrix": [5,4], "x": 8, "y": 0 },
                { "matrix": [5,3], "x": 9, "y": 0 },
                { "matrix": [5,2], "x": 10, "y": 0 },
                { "matrix": [5,1], "x": 11, "y": 0 },
                { "matrix": [5,0], "x": 12, "y": 0 },

                { "matrix": [6,5], "x": 7, "y": 1 },
                { "matrix": [6,4], "x": 8, "y": 1 },
                { "matrix": [6,3], "x": 9, "y": 1 },
                { "matrix": [6,2], "x": 10, "y": 1 },
                { "matrix": [6,1], "x": 11, "y": 1 },
                { "matrix": [6,0], "x": 12, "y": 1 },

                { "matrix": [7,5], "x": 7, "y": 2 },
                { "matrix": [7,4], "x": 8, "y": 2 },
                { "matrix": [7,3], "x": 9, "y": 2 },
                { "matrix": [7,2], "x": 10, "y": 2 },
                { "matrix": [7,1], "x": 11, "y": 2 },
                { "matrix": [7,0], "x": 12, "y": 2 },

                { "matrix": [8,5], "x": 7, "y": 3 },
                { "matrix": [8,4], "x": 8, "y": 3 },
                { "matrix": [8,3], "x": 9, "y": 3 },
                { "matrix": [8,2], "x": 10, "y": 3 },
                { "matrix": [8,1], "x": 11, "y": 3 },
                { "matrix": [8,0], "x": 12, "y": 3 },

                { "matrix": [9,4], "x": 7, "y": 4 },
                { "matrix": [9,3], "x": 8, "y": 4 },
                { "matrix": [9,2], "x": 9, "y": 4 },
                { "matrix": [9,1], "x": 10, "y": 4 },
                { "matrix": [9,0], "x": 11, "y": 4 }

                // Right thumb cluster
                { "matrix": [9,5], "x": 12, "y": 4 },
                { "matrix": [5,6], "x": 13, "y": 0 },
                { "matrix": [6,6], "x": 13, "y": 1 },
                { "matrix": [7,6], "x": 13, "y": 2 },
                { "matrix": [8,6], "x": 13, "y": 3 },
                { "matrix": [9,6], "x": 13, "y": 4 },

            ]
        }
    }
}
